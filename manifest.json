{
  "manifest_version": 3,
  "name": "BugWhisperer Launcher",
  "version": "1.0",
  "description": "Changes background color of the current page",
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["activeTab", "scripting", "contextMenus"]
}
